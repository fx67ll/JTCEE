{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		// 登录
		{
			"path": "pages/login/login",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 忘记密码
		{
			"path": "pages/login/forget_password",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 通知
		{
			"path": "pages/notice/notice_catagory",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 快递消息
		{
			"path": "pages/notice/notice_index",
			"style": {
				"navigationStyle": "custom",
				// H5开启下拉刷新，不可以自定义设置下拉样式，只可以使用circle样式  
				"enablePullDownRefresh": true,
				"h5": {
					"pullToRefresh": {
						"color": "#5bc797",
						"offset": "60px"
					}
				}
			}
		},
		// 消息详情
		{
			"path": "pages/notice/notice_detail",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 首页
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 运费计算
		{
			"path": "pages/index/express_bill",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 我的
		{
			"path": "pages/user/user_index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 个人设置
		{
			"path": "pages/user/user_setting",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 地址一览
		{
			"path": "pages/address/address_index",
			"style": {
				"navigationStyle": "custom",
				// H5开启下拉刷新，不可以自定义设置下拉样式，只可以使用circle样式
				"enablePullDownRefresh": true,
				"h5": {
					"pullToRefresh": {
						"color": "#5bc797",
						"offset": "150px"
					}
				}
			}
		},
		// 地址添加
		{
			"path": "pages/address/address_add",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 地址详情
		{
			"path": "pages/address/address_detail",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 运单一览
		{
			"path": "pages/invoice/invoice_index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 运单详情
		{
			"path": "pages/invoice/invoice_detail",
